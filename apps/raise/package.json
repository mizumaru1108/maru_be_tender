    "prebuild": "rimraf dist",
    "start": "concurrently -c auto -n prisma,nest 'prisma generate' 'nest start'",
    "start:debug": "concurrently -c auto -n prisma,nest 'prisma generate --watch' 'next start--debug --watch'",
